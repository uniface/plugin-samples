;-------------------------------------------------------
; Perform analysis
; - creates occurrences of E.N for issues found
;-------------------------------------------------------
partner operation analyzeCode
params
  string pUrl: in
endparams
variables
  string  vType, vEntity, vObjectName
  string  vObjectType, vObjectLabel, vObjectGroup, vObjectModel, vModeled
  numeric vStatus, vComponentEntityCount, vModeledEntityCount, vComponentFieldCount, vModeledFieldCount, vFirstEntity, vFirstField
  string  vComponentEntityList, vModeledEntityList, vComponentFieldList, vModeledFieldList
  string  vComponentEntityNames, vModeledEntityNames, vComponentFieldNames, vModeledFieldNames
endvariables

  $EntryPrefix$ = $logical("WS_CHECKSTYLE_ENTRY_PREFIX")
#ifdefined DEFAULT_ENTRY_PREFIX
  if ($EntryPrefix$ = "")
    $EntryPrefix$ = "&lt;DEFAULT_ENTRY_PREFIX&gt;"
  endif
#endif
  $EntryPrefixLen$ = $length($EntryPrefix$)

  $OperationPrefix$ = $logical("WS_CHECKSTYLE_OPERATION_PREFIX")
#ifdefined DEFAULT_OPERATION_PREFIX
  if ($OperationPrefix$ = "")
    $OperationPrefix$ = "&lt;DEFAULT_OPERATION_PREFIX&gt;"
  endif
#endif
  $OperationPrefixLen$ = $length($OperationPrefix$)

  vObjectType  = ""
  vObjectLabel = ""
  vObjectGroup = ""
  vObjectModel = ""
  vModeled     = ""

  vStatus = $split(pUrl, 1, ":", vType, vObjectName)
  if (vType != "cpt")
    return 0
  endif
  
  clear/e "E.N"
  clear/e "UFORM.DICT"
  ULABEL.UFORM/init = vObjectName
  retrieve/e "UFORM"
  if ($status = 0)
	call lCheckCode(ULABEL.UFORM, "", "", USCRIPT.UFORM, "USCRIPT")
	if (!$empty(UXGROUP.DICT))
		forentity "UXGROUP"
			vEntity = $concat(ULABEL.UXGROUP, ".", UBASE.UXGROUP)
			call lCheckCode(ULABEL.UFORM, vEntity, "", UCOLL_SCRIPT.UXGROUP, "UCOLL_SCRIPT")
			call lCheckCode(ULABEL.UFORM, vEntity, "", UOCC_SCRIPT.UXGROUP,  "UOCC_SCRIPT")
			if (!$empty(UXFIELD.DICT))
				forentity "UXFIELD"
					call lCheckCode(ULABEL.UFORM, vEntity, ULABEL.UXFIELD, USCRIPT.UXFIELD, "USCRIPT")
				endfor
			endif
		endfor
	endif
  endif
  clear/e "UFORM"
  setocc "E", 1

end ; analyzeCode


;------------------------------------------------------------
; Analyze a block of code and add non-conformant to list
;------------------------------------------------------------
entry lCheckCode
params
	string pComponent : IN
	string pEntity    : IN
	string pField     : IN
	string pCode      : IN
    string pType      : IN
endparams
variables
  string  vScriptList, vLine, vName, vModuleType, vIssue
  numeric vLineNo
endvariables

	vScriptList = $replace(pCODE, 1, "%%^", "&uSEP;", -1)
	forlist vLine, vLineNo in vScriptList
		vIssue = ""
		if ($EntryPrefix$ != "" | $OperationPrefix$ != "")
			call lGetModuleName(vLine, vName, vModuleType)
			if (vName != "")
				selectcase vModuleType
					case "entry"
						if ($EntryPrefixLen$ &gt; 0 &amp; vName[1:$EntryPrefixLen$] != $EntryPrefix$)
							vIssue = $concat("entry name should start with ", $EntryPrefix$)
						endif
					case "operation"
						if ($OperationPrefixLen$ &gt; 0 &amp; vName[1:$EntryPrefixLen$] != $OperationPrefix$)
							vIssue = $concat("operation name should start with ", $OperationPrefix$)
						endif
				endselectcase
			endif
		endif
		if (vIssue != "")
			creocc "E.N", -1
			;debug
			if (pEntity = "")
				; Component trigger
				LOC.E = $concat($URL$, %\
						$concat("/prop:", pType), %\
						$concat("/pos:",  vLineNo, ",1"))
			elseif (pField = "")
				; Entity trigger
				LOC.E = $concat($URL$, %\
					    $concat("/ent:",  pEntity), %\
					    $concat("/prop:", pType), %\
		                $concat("/pos:",  vLineNo, ",1"))
			else
				; Field trigger
				LOC.E = $concat($URL$, $concat("/ent:", pEntity), %\
		                $concat("/fld:",  pField), %\
	                    $concat("/prop:", pType), %\
		                $concat("/pos:",  vLineNo, ",1"))
			endif
			ISSUE.E  = vIssue
			SCRIPT.E = vLine
			;Examples of output formatting with color
			;if (pModeled = "modeled")
			;  $fieldproperties(label.e) = "BACKCOLOR=cornflowerblue&uSEP;FORECOLOR=white"
			;endif
			;if (pField != "")
			;  $fieldproperties(FLD.E) = "FORECOLOR=darkblue"
			;elseif (ENT.E != "")
			;  $fieldproperties(ENT.E) = "FORECOLOR=darkblue"
			;else
			;  $fieldproperties(CPT.E) = "FORECOLOR=darkblue"
			;endif
			;$fieldproperties(LOC.E)   = "FORECOLOR=darkblue"
			;$fieldproperties(ISSUE.E) = "FORECOLOR=red"
		endif
	endfor

end ; lCheckCode


;------------------------------------------------------------
; Analyze a line of code and return module name if applicable
; Note: currently only parses entry and operation
;       could be enhanced to return triggers
;------------------------------------------------------------
entry lGetModuleName
params
	string pLine       : IN
	string pName       : OUT
	string pModuleType : OUT
endparams
variables
	string  vChar
	numeric vLen, vPos
endvariables

	vLen = $length(pLine)
	if (vLen &gt; 0)
		; Ignore leading whitespace
		vPos = 1
		while (vPos &lt; vLen)
			vChar = pLine[vPos:1]
			if (vChar != " " &amp; vChar != "	") break
			vPos += 1
		endwhile
		; Check if entry or operation
		if (pLine[vPos:5] = 'entry')
			pModuleType = "entry"
			pName = lExtractModuleName(pLine, vLen, vpos+5)
		elseif (pLine[vPos:9] = 'operation')
			pModuleType = "operation"
			pName = lExtractModuleName(pLine, vLen, vpos+9)
		endif
	endif

end ; lpGetEntryName


;-----------------------------------------
; Extract module name from a line of code
;-----------------------------------------
entry lExtractModuleName
returns string
params
	string  pLine : IN
	string  pLen  : IN
	numeric pPos  : IN
endparams
variables
	string  vChar, vName
	numeric vPos
endvariables

	if ((pLine[pPos:1] = " " | pLine[pPos:1] = "	"))
		pPos += 1
		; Ignore whitespace between module and name, and stop at comment
		vChar = pLine[pPos:1]
		while (pPos &lt; pLen &amp; (vChar = " " | vChar = "	") &amp; vChar != ";")
			pPos += 1
			vChar = pLine[pPos:1]
		endwhile
		; Extract name
		vPos = pPos
		while (vPos &lt; pLen &amp; vChar != " " &amp; vChar != "	" &amp; vChar != ";")
			vPos += 1
			vChar = pLine[vPos]
		endwhile
		vName = $ltrim(pLine[pPos, vPos], " ")
	endif
	return vName

end ; lExtractModuleName
