<DAT name="USCRIPT" xml:space='preserve'>;------------------------------------------
; Return API version: Mandatory operation
;------------------------------------------
operation getVersion
params
  numeric pApiVersion : OUT
endparams

  pApiVersion = 1

end ; getVersion


;-------------------------------------------------------
; Return Worksheet Title &amp; Toolip - Mandatory operation
;-------------------------------------------------------
operation getWorkSheetOptions
params
  struct pWorkSheet : OUT
endparams

  pWorkSheet = $newstruct
  pWorkSheet-&gt;title = "Listing"
  pWorkSheet-&gt;tooltip = "View compile listing"

end ; getWorkSheetOptions


;-------------------------------------------------------
; Activate Worksheet - Mandatory operation
;-------------------------------------------------------
operation ws_activate
params
  $URL$ : IN
endparams

variables
	string  vOutput, vType, vLabel
	numeric vStatus
endvariables

  vStatus = $split($URL$, 1, ":", vType, vLabel)

#ifdefined TRACE
  activate "IDE_API_1".outputMessage(vOutput)
  if ($procerror != 0)
    putmess "Activate error: %%$procerror%%%, %%$procerrorcontext%%%"
  endif
#endif

  putitem/id $properties(LISTING.E), "Toolbar", "Font"
  putitem/id $properties(LISTING.E), "Font", "Consolas"
  edit/nonmodal

end ; ws_activate


;-------------------------------------------------------
; Disable Accept and Quit triggers
;-------------------------------------------------------
trigger accept
return -1
end

trigger quit
return -1
end

;-------------------------------------------------------
; Worksheet Visible: optional
;-------------------------------------------------------
operation ws_getsVisible

  call lLoadListing

#ifdefined TRACE
  activate "IDE_API_1".outputMessage("Worksheet Gets Visible (instance=%%$instancename)")
  if ($procerror != 0)
    putmess "Activate error: %%$procerror%%%, %%$procerrorcontext%%%"
  endif
#endif
  
end ; ws_getsVisible


;-------------------------------------------------------
; Worksheet invisible: optional
;-------------------------------------------------------
operation ws_getsInVisible

#ifdefined TRACE
  activate "IDE_API_1".outputMessage("Worksheet Gets InVisible (instance=%%$instancename)")
  if ($procerror != 0)
    putmess "Activate error: %%$procerror%%%, %%$procerrorcontext%%%"
  endif
#endif

end ; ws_getsInVisible


;-------------------------------------------------------
; Refresh: optional
;-------------------------------------------------------
operation ws_refresh

  call lLoadListing
 
#ifdefined TRACE
  activate "IDE_API_1".outputMessage("Refresh Worksheet requested (instance=%%$instancename)")
  if ($procerror != 0)
    putmess "Activate error: %%$procerror%%%, %%$procerrorcontext%%%"
  endif
#endif
  
end ; ws_refresh


;-------------------------------------------------------
; Object Renamed: optional
;-------------------------------------------------------
operation ws_objectRenamed
params
  $URL$ : IN
endparams

#ifdefined TRACE
  activate "IDE_API_1".outputMessage("Main Udo renamed (Url=%%$URL$%%%).")
  if ($procerror != 0)
    putmess "Activate error: %%$procerror%%%, %%$procerrorcontext%%%"
  endif
#endif

end ; ws_objectRenamed


;---------------------------------------------------------------------------------------------
; Load Listing
;---------------------------------------------------------------------------------------------
entry lLoadListing

  fileload $concat($URL$[5],".cptlst") , LISTING.E
  if ($procerror != 0)
	if ($procerror = -4)
		activate "IDE_API_1".outputMessage("Listing for %%$URL$%%% not found")
	else
		activate "IDE_API_1".outputMessage("Error loading listing for %%$URL$%%%: %%$procerrorcontext%%%")
		LISTING.E = "Error loading listing: %%$procerrorcontext%%%"
	endif
  endif

end ; lLoadListing
