;-------------------------------------------------------
; Perform analysis
;-------------------------------------------------------
partner operation getGridData
params
  string pUrl: in
  string pOutput: out
endparams
variables
  string vType, vObjectName
  string vObjectType, vObjectLabel, vObjectGroup, vObjectModel, vModeled
  numeric vObjectLinesModel,vObjectLinesCpt
  numeric vStatus, vComponentEntityCount, vModeledEntityCount, vComponentFieldCount, vModeledFieldCount, vFirstEntity, vFirstField
  string vComponentEntityList, vModeledEntityList, vComponentFieldList, vModeledFieldList
  string vComponentEntityNames, vModeledEntityNames, vComponentFieldNames, vModeledFieldNames
endvariables

  vObjectType  = ""
  vObjectLabel = ""
  vObjectGroup = ""
  vObjectModel = ""
  vModeled     = ""
  vObjectLinesModel = 0 
  vObjectLinesCpt   = 0

  vStatus = $split(pUrl, 1, ":", vType, vObjectName)
  if (vType != "cpt")
    return 0
  endif
  ;debug

  clear/e "E.N"
  clear/e "UFORM.DICT"
  ULABEL.UFORM/init = vObjectName
  retrieve/e "UFORM"
  if ($status = 0)
    vObjectType = "Component"
    vObjectLabel = ULABEL.UFORM
;	if ($dbocc(uform_template) = 1)
;		vObjectModel = ulabel.uform_template
;		vModeled = "modeled"
;		vObjectLinesModel = lGetLines("cpt","model")
;	endif
    vObjectLinesCpt = lGetLines("cpt","cpt")
    call lCreateOcc(vObjectType, vObjectLabel, vObjectGroup, vObjectModel, vModeled, vObjectLinesModel, vObjectLinesCpt)

    vComponentEntityCount = $hits(UXGROUP)
    if (vComponentEntityCount &gt; 0)
      vFirstEntity = 1
    endif
    forentity "UXGROUP"
      vObjectType = "Entities"
      vObjectLabel = ULABEL.UXGROUP
      vObjectModel = UBASE.UXGROUP
      putitem/id vComponentEntityList, $concat(vObjectModel, ".", vObjectLabel), $concat(vObjectLabel, ".", vObjectModel)
      setocc "UCGROUP" , 1
      if ($dbocc(UCGROUP) = 1)
		vModeled = "modeled"
        vObjectLinesModel = lGetLines("ent","model")
      endif

      vObjectLinesCpt  = lGetLines("ent","cpt")
	  if (vFirstEntity = 1)
        vObjectType = "%%vObjectType%%% (%%vComponentEntityCount%%%)"
        vFirstEntity = 0
      else
        vObjectType =""
      endif
      call lCreateOcc(vObjectType, vObjectLabel, vObjectGroup, vObjectModel, vModeled, vObjectLinesModel, vObjectLinesCpt)
    endfor

    vComponentFieldCount = $hits(UXFIELD)
    if (vComponentFieldCount &gt; 0)
      vFirstField = 1
    endif
    forentity "UXFIELD" 
      vObjectType = "Fields"
      vObjectLabel = ULABEL.UXFIELD
      vObjectGroup = GRP.UXFIELD
      vObjectModel = UBASE.UXFIELD
      setocc "UCFIELD" , 1
      if ($dbocc(UCFIELD) = 1)
        vModeled = "modeled"
        vObjectLinesModel = lGetLines("fld","model")
      endif

      vObjectLinesCpt   = lGetLines("fld","cpt")
      if (vFirstField = 1)
        vObjectType = "%%vObjectType%%% (%%vComponentFieldCount%%%)"
        vFirstField = 0
      else
        vObjectType =""
      endif
      call lCreateOcc(vObjectType, vObjectLabel, vObjectGroup, vObjectModel, vModeled, vObjectLinesModel, vObjectLinesCpt)
    endfor
  endif
  clear/e "UFORM"
  setocc "E", 1
  ;$entityproperties(E) = "ColumnWidth=Type=80px!;Label=80px!;Group=80px!;Model=80px!;LinesModel=50px!;LinesCpt=50px"
  ;pOutput = $entityproperties(E)

end ; getGridData


;-------------------------------------------------------
; Return total line count
;-------------------------------------------------------
entry lGetLines 
returns numeric
params
  string pType: 	in
  string pLevel: 	in
  ;string pLabel: 	in
  ;string pGroup: 	in
  ;string pBase:	in
endparams
variables
  numeric vLines
endvariables

  vLines = 0
  selectcase pType
    case "cpt" ; component
      selectcase pLevel
;       case "model" ; from template
;         vLines += lGetLineCount(udeclarations.uform_template)
;         vLines += lGetLineCount(uscript.uform_template)
        case "cpt" 	; from component
          vLines += lGetLineCount(udeclarations.uform)
          vLines += lGetLineCount(uscript.uform)
      endselectcase
    case "ent"
      selectcase pLevel
        case "model" ; from modeled entity
          vLines += lGetLineCount(udeclarations.ucgroup)
          vLines += lGetLineCount(ucoll_script.ucgroup)
          vLines += lGetLineCount(uocc_script.ucgroup)
        case "cpt"	; from component entity
          vLines += lGetLineCount(udeclarations.uxgroup)
          vLines += lGetLineCount(ucoll_script.uxgroup)
          vLines += lGetLineCount(uocc_script.uxgroup)
      endselectcase
    case "fld"
      selectcase pLevel
        case "model" ; from modeled field
          vLines += lGetLineCount(udeclarations.ucfield)
          vLines += lGetLineCount(uscript.ucfield)
        case "cpt"	; from component field
          vLines += lGetLineCount(udeclarations.uxfield)
          vLines += lGetLineCount(uscript.uxfield)
      endselectcase
  endselectcase

  return vLines

end ; lGetLines


;-----------------------------------------------------
; Get Line Count for a specific script or declaration
;-----------------------------------------------------
entry lGetLineCount
returns numeric
params
  string pContainer: in
endparams
variables
  numeric vLineCount, vPos, vEndFound
  string vLine, vContainerText
endvariables

  vLineCount = 0

  if ($length(pContainer)) = 0
	return vLineCount
  endif

  vContainerText = pContainer

  vEndFound = 0

  while (vEndFound = 0)
	vPos = $scan(vContainerText, "%%^")	
	if (vPos &gt; 0)
		vLine = vContainerText[1:vPos-1]
		vContainerText = vContainerText[vPos+1] 
	else
		vLine = vContainerText 
		vEndFound = 1
	endif
	vLine = $lowercase($ltrim($rtrim($replace(vLine, 1,$string("&amp;uTAB;")," ",-1), " "), " "))
	if ($length(vLine) &gt; 0) 
		if (vLine[1:1] != ";")
			vLineCount = vLineCount +1
		endif
	endif
  endwhile

  return vLineCount

end ; lGetLineCount


;-----------------------------------------
; Create an occurrence of the grid entity
;-----------------------------------------
entry lCreateOcc
params
	string 	pObjectType: 		inout
	string 	pObjectLabel:		inout
	string 	pObjectGroup: 		inout
	string 	pObjectModel:		inout
	string 	pModeled:			inout
	numeric pObjectLinesModel:	inout
	numeric pObjectLinesCpt:	inout
endparams

  creocc "E.N", -1
  type.e       = pObjectType
  label.e      = pObjectLabel
  group.e      = pObjectGroup
  model.e      = pObjectModel
  linesmodel.e = pObjectLinesModel
